%5







gammafun = @(x) (t^(x-1))*e^(-t);








type gammaests


clear x


difmat = zeros(21,2);
difmat(:,1) = linspace(0,20,21)';



for i = 0:20
    x=i;
    gammafun = @(t) (t.^(x-1)).*exp(-t);
    dif = quadgk(gammafun,0,inf) - gamma(x);
    difmat(i+1,2) = dif;
end

